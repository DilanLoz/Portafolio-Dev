---
import ExperienceItem from "../components/ExperienceItem.astro"

const EXPERIENCE = [
    {
        date: 'Actualmente...',
        title: 'Desarrollador Junior y Creador de contenido',
        description: 'Soy desarrollador y creador de contenido de programacion ',
        link: '#'  
    },
    {
        date: 'February 2023 - Present',
        title: 'Software Engineer',
        description: 'Software Engineer',
        link: '#'  
    }
]
---
<ol class="relative border-s border-gray-200 ml-2.5">
    {
        EXPERIENCE.map(experience => (
            <li class="mb-10 ms-4">
                <ExperienceItem {...experience}/>
            </li>

        ))
    }                  
</ol>

